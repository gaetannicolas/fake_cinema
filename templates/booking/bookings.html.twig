{% extends 'base.html.twig' %}

{% block title %}Fake Cinema{% endblock %}

{% block body %}
<h1>Bookings</h1>
    <ul>
    {% for booking in bookings %}

        <li>
            <p>{{ booking.screeningId }}</p>
            <p>{{ booking.screeningId.filmId }}</p>
            <a href="{{ path('deleteBooking', {'id': booking.id}) }}">Delete</a>
        </li>

    {% endfor %}
    </ul>

{% endblock %}
